.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  padding: 20rpx;
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index-floor .floor-list {
  padding: 10rpx 0;
}
.index-floor .floor-list .floor-title image {
  width: 100%;
}
.index-floor .floor-list .floor-content {
  overflow: hidden;
}
.index-floor .floor-list .floor-content navigator {
  float: left;
  width: 33.33%;
}
.index-floor .floor-list .floor-content navigator:nth-last-child(-n+4) {
  height: calc(12865.38vw / 232 / 2);
  border-left: 10rpx solid #ffffff;
}
.index-floor .floor-list .floor-content navigator:nth-child(2),
.index-floor .floor-list .floor-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index-floor .floor-list .floor-content navigator image {
  width: 100%;
  height: 100%;
}
